{
  "name": "geohash-kit",
  "version": "1.2.0",
  "description": "Modern TypeScript geohash toolkit â€” encode, decode, cover polygons, and build Nostr filters",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    },
    "./coverage": {
